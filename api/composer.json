{
    "repositories": [
        {
            "type": "gitlab",
            "url": "https://gitlab.cyrextech.net/libraries/laravel-mattermost"
        },
        {
            "type": "gitlab",
            "url": "https://gitlab.cyrextech.net/libraries/Elasticquent"
        }
    ],

    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.7",
        "ext-intl": "*",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^2.1",
        "barryvdh/laravel-snappy": "^1.0",
        "crazybooot/base64-validation": "^1.0",
        "darkaonline/l5-swagger": "^8.6",
        "dompdf/dompdf": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "laravel/framework": "^7.0",
        "laravel/telescope": "^3.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "maatwebsite/excel": "^3.1",
        "mogi/elasticquent": "dev-master",
        "mogi/laravel-mattermost": "dev-master",
        "php-http/message-factory": "^1.1",
        "pragmarx/google2fa": "^8.0",
        "sentry/sentry-laravel": "^1.8",
        "spatie/data-transfer-object": "^2.8",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-enum": "^1.6",
        "spatie/laravel-medialibrary": "^8.3",
        "spatie/laravel-server-monitor": "^1.9",
        "tenancy/affects-connections": "^1.0",
        "tenancy/db-driver-mysql": "^1.0",
        "tenancy/framework": "^1.0",
        "tenancy/hooks-database": "^1.0",
        "tenancy/hooks-migration": "^1.0",
        "tenancy/identification-driver-console": "^1.0",
        "tenancy/identification-driver-http": "^1.0",
        "tenancy/identification-driver-queue": "^1.0",
        "tymon/jwt-auth": "^1.0",
        "webfox/laravel-xero-oauth2": "^2.0",
        "whichbrowser/parser": "^2.1",
        "xeroapi/xero-php-oauth2": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "gitlab-domains": [
            "gitlab.cyrextech.net"
        ],
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Enums\\": "app/Enums"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "packages/phpdocx/Classes/Phpdocx"
        ],
        "files": [
            "app/Extra/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "phpcs --standard=ruleset.xml app --colors"
        ],
        "autofix": [
            "phpcbf --standard=ruleset.xml app --colors"
        ]
    }
}
